genrule(
    name = "bar",
    out = "bar",
    bash = "touch $OUT",
    cmd_exe = "echo \"\" > %OUT%",
)

genrule(
    name = "bar_with_multiple_outputs",
    outs = {
        "output1": [
            "bar",
        ],
        "output2": [
            "baz",
        ],
    },
    bash = "touch $OUT/bar && touch $OUT/baz",
    cmd_exe = "(echo something)>%OUT%\\bar && (echo something)>%OUT%\\baz",
)

genrule(
    name = "bar_with_multiple_outputs_but_really_only_has_one_heehee",
    outs = {
        "output1": [
            "bar",
        ],
    },
    bash = "touch $OUT/bar",
    cmd_exe = "(echo something)> %OUT%\\bar",
)

genrule(
    name = "foo",
    srcs = [
        ":bar",
    ],
    out = "foo",
    cmd = "touch $OUT",
    cmd_exe = "echo \"\" > %OUT%",
)

genrule(
    name = "ex ample",
    srcs = [
        ":bar",
    ],
    out = "example",
    cmd = "touch \"$OUT\"",
    cmd_exe = "echo \"\" > \"%OUT%\"",
)
